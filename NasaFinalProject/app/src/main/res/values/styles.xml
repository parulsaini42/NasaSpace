<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#161616</item>
        <item name="colorPrimaryDark">#161616</item>
        <item name="colorAccent">@null</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.Launcher" parent="Theme.AppCompat.Light.NoActionBar"></style>
    <style name="Autocomplete" parent="Widget.AppCompat.Light.AutoCompleteTextView">
        <item name="android:background">#d8d3d3</item>
        <item name="colorControlNormal">#fbf9f9</item>
        <item name="colorControlActivated">#000</item>
    </style>
</resources>
